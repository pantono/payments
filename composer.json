{
  "name": "pantono/payments",
  "description": "Payments Module for Pantono",
  "type": "library",
  "require": {
    "php": ">=8.2",
    "symfony/console": "^7.0",
    "pantono/database": "^1.0",
    "pantono/contracts": "^1.0",
    "pantono/hydrator": "^1.0",
    "symfony/event-dispatcher": "^7.0",
    "robmorgan/phinx": "^0.16.0",
    "pantono/queue": "^1.0",
    "league/omnipay": "^3.2",
    "omnipay/paypal": "^3.0",
    "stripe/stripe-php": "^16.2",
    "pantono/config": "^1.0",
    "pantono/core": "^1.0",
